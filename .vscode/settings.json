{
  "editor.rulers": [80],
  "editor.formatOnSave": true,
  "prettier.tabWidth": 2,
  "editor.tabSize": 2,
  "editor.formatOnSaveTimeout": 1500,
  "editor.snippetSuggestions": "top",
  "peacock.color": "cyan",
  "workbench.colorCustomizations": {
    "titleBar.activeBackground": "#0000ff",
    "titleBar.inactiveBackground": "#2c2cfa7d",
    "titleBar.activeForeground": "#ffffff",
    "titleBar.inactiveForeground": "#a9a9a9e5",
    "statusBar.background": "#0000ff",
    "statusBarItem.hoverBackground": "#6c59ff",
    "statusBar.foreground": "#dbedff"
  },
  "peacock.affectActivityBar": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "cSpell.words": ["postbuild", "predeploy"]
}
